import java.awt.Color;
import java.awt.FlowLayout;

import javax.swing.*;

class GUI{
	
	public static void genererGUI() {
		JFrame window = new JFrame("Projet d'Ã©lectronique digitale");
	       window.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
	       window.setSize(500,300);
	       String[] ports = { "COM1", "COM2"};
	       
	       JComboBox choixPort = new JComboBox(ports);
	       choixPort.setSelectedIndex(1);
	      
	       JButton press = new JButton();
	       press.setBackground(Color.LIGHT_GRAY);
	       press.setFocusPainted(false);
	       press.setBorder(null);
	       press.setText("Envoyer");
	       window.add(press);
	       
	       window.setVisible(true);
	}
    public static void main(String args[]){
       genererGUI()
    }
}